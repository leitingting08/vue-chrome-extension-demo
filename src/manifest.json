{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "homepage_url": "https://github.com/leitingting08/vue-chrome-extension-demo",
  "description": "A Vue Browser Extension Demo",
  "default_locale": "en",
  "permissions": [
    "tabs",
    "contextMenus",
    "notifications",
    "activeTab",
    "<all_urls>",
    "*://*/*"
  ],
  "icons": {
    "16": "icons/em.png",
    "48": "icons/em.png",
    "128": "icons/em.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "devtools_page": "devtools.html",
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "__MSG_extName__",
    "default_icon": {
      "19": "icons/ali.png",
      "38": "icons/ali.png"
    }
  },
  "omnibox": { "keyword" : "go" },
  "options_ui": {
    "page": "options.html",
    "browser_style": true
  },
  "chrome_url_overrides":{
    "newtab": "override.html"
  }
}
